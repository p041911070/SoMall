@page
@inherits TT.SoMall.Pages.SoMallPage
@using TT.SoMall.Pages.Shops
@model IndexModel

@section scripts
{
    <abp-script src="/Pages/Shops/index.js"/>
}

<abp-card>
    <abp-card-header>
        <h2>@L["Shops"]</h2>Shops
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="ShopsTable">
            <thead>
            <tr>
                <th>name</th>
                <th>shortName</th>
                <th>logoImage</th>
            </tr>
            </thead>
        </abp-table>
    </abp-card-body>
</abp-card>